                    <ui:Label text="VIGNETTE" enable-rich-text="false" name="Vignette" style="font-size: 40px; -unity-text-align: middle-right; align-self: stretch; flex-grow: 1; flex-shrink: 1; align-items: center; justify-content: center; color: rgb(255, 191, 0);"/>
                    <ui:Label text="TONEMAPPING" name="Tonemapping" enable-rich-text="false" style="font-size: 40px; -unity-text-align: middle-right; align-self: stretch; flex-grow: 1; flex-shrink: 1; align-items: center; justify-content: center; color: rgb(255, 191, 0);"/>
